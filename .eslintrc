{
  "extends": "@callstack",
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto",
        "arrowParens": "avoid",
        "semi": false
      }
    ],
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}