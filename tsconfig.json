{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "jsx": "react-native",
    "typeRoots": [
      "./src/types"
    ],
    "paths": {
      "@sinonavo/components": [
        "./src/components/index.ts"
      ],
      "@sinonavo/types": [
        "./src/types/index.d.ts"
      ],
      "@sinonavo/stores/*": [
        "./src/stores/*.ts"
      ],
      "@sinonavo/hooks/*": [
        "./src/hooks/*.ts"
      ],
      "@sinonavo/global": [
        "./src/global/index.ts"
      ]
    }
  }
}